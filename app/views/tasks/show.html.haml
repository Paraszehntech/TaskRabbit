#jobs
    .jobs

        %h1= @task.title
        %p= @task.description
        %p= @task.company
        %button.btn.btn-default= link_to "Complete Task" , @task.url
#links
    = link_to "Home",root_path , class: "btn btn-sm btn-default"
    = link_to "Edit", edit_task_path(@task), class: "btn btn-sm btn-default"
    = link_to 'Delete', @doc, data: { turbo_method: 'delete', turbo_confirm: "Are you sure" }, class: "btn btn-outline-danger btn btn-sm btn-default"
    